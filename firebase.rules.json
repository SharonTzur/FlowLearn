{
  "rules": {
    "users": {
      ".read": true,
      ".write": true,
      "$uid": {
        ".read": true,
        ".write": true
      }
    },
    "tasks": {
      "$uid": {
        ".read": true,
        ".write": true,
        ".indexOn": [
          "completed"
        ]
      }
    }
  }
}
